{
  "name": "api_boiler_plate",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "serve": "npm install && npm run build && node ./dist/server.js",
    "build": "npm run clear && babel src --out-dir dist --copy-files",
    "test": "jest --watch",
    "clear": "rimraf ./dist",
    "start": "babel-watch ./src/server.js"
  },
  "author": "me",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "change-case": "^3.0.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "express-list-routes": "^0.1.4",
    "mongoose": "^4.11.13",
    "morgan": "^1.9.0",
    "require-dir": "^0.3.2",
    "require-directory": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-watch": "^2.0.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "mockingoose": "^2.3.0",
    "prettier-eslint": "^8.2.0",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist",
      "./helpers",
      "./data"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/controllers/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/models"
    ]
  }
}
